{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", "*.tsbuildinfo"]
        },
        "lint": {
            "outputs": []
        },
        "test": {
            "dependsOn": ["^build"],
            "outputs": []
        }
    }
}
