{
    "name": "vite-plugin-envsubst",
    "version": "0.2.0",
    "description": "Vite plugin that transforms environmental variables into a global statement that can be replaced with `envsubst` or Caddy's templating engine.",
    "keywords": [
        "caddy",
        "docker",
        "kubernetes",
        "nginx",
        "vite-plugin"
    ],
    "homepage": "https://github.com/abhiaagarwal/vite-plugin-envsubst",
    "bugs": {
        "url": "https://github.com/abhiaagarwal/vite-plugin-envsubst/issues"
    },
    "license": "(MIT OR Apache-2.0)",
    "author": "Abhi Agarwal <abhiaagarwal01@gmail.com> (https://abhi.rodeo)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/abhiaagarwal/vite-plugin-envsubst.git"
    },
    "files": [
        "dist"
    ],
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs"
        }
    },
    "scripts": {
        "build": "tsc && unbuild",
        "test": "vitest run"
    },
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "@types/bun": "^1.1.14",
        "@types/node": "^22.10.2",
        "eslint": "^9.17.0",
        "oxfmt": "^0.21.0",
        "typescript": "~5.6.3",
        "typescript-eslint": "^8.18.1",
        "unbuild": "^3.0.1",
        "vitest": "4"
    },
    "peerDependencies": {
        "vite": "*"
    }
}
